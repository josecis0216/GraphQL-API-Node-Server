datasource db {
  provider = "postgresql"
  url      = env("DB_CONNECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Player {
  id           String   @default(cuid()) @id
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  name         String
  position     String
  dob          String
  nationality  String
  currentTeam  String
  jerseyNumber String
}